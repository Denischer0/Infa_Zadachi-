Задача 1 

#include <iostream>
using namespace std;
int main() {
	float x, y;
	cout << "Write the first day`s money" << endl;
	cin >> x;
	cout << "Write yhe amount of money we need to let Ivan Ivanovich visit a prison" << endl;
	cin >> y;
	int k = 1;
	while (x < y) {
		k++;
		x = x * 1.1;

	}
	cout << "Ivan Ivanovich will be punished at the day " << k;
}
