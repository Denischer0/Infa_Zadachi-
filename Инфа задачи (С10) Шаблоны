Задача 1

#include <iostream>
using namespace std;

void swapValues(int a, int b) {
	int c = a;
	a = b;
	b = c;
	cout << a << " " << b;
}
void swapValues(double a, double b) {
	double c = a;
	a = b;
	b = c;
	cout << a << " " << b;
}

int main() {
	int a = 5, b = 3;
	double c = 3.14, d = 5.12;
	swapValues(a, b);
	cout << endl;
	swapValues(c, d);	
}

Задача 2

#include <iostream>
using namespace std;

void myMin(int a, int b) {
	if (a < b) {
		cout << a;
	}
	else {
		cout << b;
	}
}
void myMin(double a, double b) {
	if (a < b) {
		cout << a;
	}
	else {
		cout << b;
	}
}
void myMin(char a, char b) {
	if (a < b) {
		cout << a;
	}
	else {
		cout << b;
	}
}
int main() {
	int a = 5, b = 3;
	double c = 3.14, d = 2.15;
	char x = 'b', y = 'a';
	myMin(a, b);
	cout << endl;
	myMin(c, d);
	cout << endl;
	myMin(x, y);
}

Задача 3

#include <iostream>
#include <vector>
using namespace std;

void sumArray(int A[], int l) {
	int s=0;
	for (int x = 0; x < l; x++) {
		s = s + A[x];
	}
	cout << s;
}
void sumArray(double A[], int l) {
	double s = 0;
	for (int x = 0; x < l; x++) {
		s = s + A[x];
	}
	cout << s;
}

int main() {
	int arr[] = { 1,2,3,4,5 };
	double Arr[] = { 1.1,1.2,1.3,1.4,1.5 };
	sumArray(arr, 5);
	cout << endl;
	sumArray(Arr, 5);
}


Задача 4

#include <iostream>
#include <vector>
#include <string>
using namespace std;

void contains(int A[], int l, int x) {
	int f = 0;
	for (int i = 0; i < l; i++) {
		if (A[i] == x) {
			f = 1;
			break;
		}
	}
	if (f == 1) {
		cout << "True";
	}
	else {
		cout << "False";
	}
}
void contains(string A[], int l, string x) {
	int f = 0;
	for (int i = 0; i < l; i++) {
		if (A[i] == x) {
			f = 1;
			break;
		}
	}
	if (f == 1) {
		cout << "True";
	}
	else {
		cout << "False";
	}
}

int main() {
	int arr[] = { 1,2,3,4,5,6,7,8,9 };
	string A[] = { "dog","cat","elephant" };
	contains(arr, 9, 12);
	cout << endl;
	contains(A, 3, "cat");
		
}
