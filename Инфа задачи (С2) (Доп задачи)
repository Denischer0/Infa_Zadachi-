Задачи 1,2 - Смотреть "Инфа задачи (С1)"

Задача 3

#include <iostream>
#include <string>
using namespace std;
int main() {
	string A; 
	cout << "Write the number A" << endl;
	cin >> A;
	cout << A[A.size() - 2];
}

Задача 4

#include <iostream>
using namespace std;
int main() {
	int A;
	cout << "Write the number A" << endl;
	cin >> A;
	cout << A + 2 - A % 2;
}

Задача 5 - Смотреть "Инфа задачи (С1)"

Задача 6 

#include <iostream>
using namespace std;
int main() {
	int A, B;
	cout << "Write the number A" << endl;
	cin >> A;
	cout << "Write the number B" << endl;
	cin >> B;
	swap(A, B);
	cout << A << " " << B;

}

Задача 7

#include <iostream>
using namespace std;
int main() {
	int a, b, h,k=0,hh=0;
	cout << "Write the hight of the pillar" << endl;
	cin >> h;
	cout << "Write the number a" << endl;
	cin >> a;
	cout << "Write the number b" << endl;
	cin >> b;
	while (hh < h) {
		k++;
		if (hh + a > h) {
			hh = hh + a;
			cout << k;
			break;
		}
		else {
			hh = hh + a - b;
		}

	}
}

Задача 8 

#include <iostream>
#include <string>
using namespace std;
int main() {
	string s;
	cin >> s;
	cout << stoi(to_string(s[0])) % stoi(to_string(s[size(s)-1])) + stoi(to_string(s[1])) % stoi(to_string(s[size(s) - 2])) + 1;
}

Задача 9

#include <iostream>
#include <string>
using namespace std;
int main() {
	int n, m;
	cout << "Write the number n" << endl;
	cin >> n;
	cout << "Write the number m" << endl;
	cin >> m;
	int ma = max(m, n);
	int mi = min(m, n);
	cout << ma % mi + 1;

}

Задача 10 

#include <iostream>
#include <string>
using namespace std;
int main() {
	int a, b;
	cout << "Write the number a" << endl;
	cin >> a;
	cout << "Write the number b" << endl;
	cin >> b;
	int mi = min(a, b);
	cout << a + b - mi;

}

(про функцию min в условии не слова btw)

